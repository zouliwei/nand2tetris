 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=u);
    Not(in=b, out=v);
    And(a=u, b=v, out=w);
    Not(in=w, out=out);
}
